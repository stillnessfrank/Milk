        <!-- end wizard step-1 -->
        <!-- begin wizard step-2 -->
        <div>
        <fieldset>
        <!-- begin row -->
        <div class="row chanquan" style="display: block;">
        <form class="form-horizontal " id="form-propertyinfo">
        <input type="hidden" value="" name="propertyId"/>
        <input type="hidden" value="" name="id"/>
        <!-- begin col-6 -->
        <div class="col-md-12">
        <!-- begin panel -->
        <div class="panel panel-inverse" data-sortable-id="form-stuff-1">
        <div class="panel-body">

        <h4>权利属性：</h4>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">产证编号:</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="ownershipCertificateCode" id="ownershipCertificateCode"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">权利人:</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="propertyOwner" id="propertyOwner"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">房地地址:</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="certificateAddress" id="certificateAddress" placeholder="房证地址信息"/>
                    </div>
                </div>
            </div>
        </div>
        <h4>土地状况：</h4>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">地号:</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="landNo" id="landNo"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">图号:</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="drawing" id="drawing"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">权属性质:</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="legalNature" id="legalNature"/>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">使用权来源<br/>/取得方式：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="sourceOfRight" id="sourceOfRight"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">土地用途：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="landUsage" id="landUsage"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">使用期限：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="useDuration" id="useDuration"/>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">宗地（丘）面积：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="landArea" id="landArea"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">使用权面积（独用）：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="independentArea" id="independentArea"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">使用权面积（分摊）：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="shareArea" id="shareArea"/>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">共用面积：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="publicArea" id="publicArea"/>
                    </div>
                </div>
            </div>
        </div>
        <h4>房屋状况：</h4>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">所有权性质：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="natureOfOwnership" id="natureOfOwnership"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">总建筑面积：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="overallBuildingArea" id="overallBuildingArea"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">幢号：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="buildingNo" id="buildingNo"/>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">建筑面积：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="buildingArea" id="buildingArea"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">部位：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="part" id="part"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">类型：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="buildingType" id="buildingType"/>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">房屋用途：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="buildingUsage" id="buildingUsage"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">结构：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="architecture" id="architecture"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">层数：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="floor" id="floor"/>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">竣工日：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="completionDate" id="completionDate"/>
                    </div>
                </div>
            </div>
        </div>
        <h4>其他信息：</h4>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">发证日：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="issueDate" id="issueDate"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">存放单位：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="storageUnit" id="storageUnit"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">管理单位：</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="managementUnit" id="managementUnit"/>
                    </div>
                </div>
            </div>
        </div>
        <h4>附件：</h4>
        <div class="form-group">
            <div class="col-md-4">
                <label class="col-md-4 control-label">上传附件：</label>
                <div class="col-md-8" style="padding-left: 0;">
                    <input type="file"/>
                </div>
            </div>
        </div>
        <div class="from-group">
            <label class="col-md-1 control-label"></label>
            <div class="col-md-11" style="padding-left: 40px;">
                <div class="image" style="width: 140px; overflow: hidden; float: left; margin-right: 10px;">
                    <div class="image-inner">
                        <a href="javascript:;" data-lightbox="gallery-group-1">
                            <img src="/theme/assets/img/fujian.png" alt="" style="width: 140px; height: 180px;"/>
                        </a>
                    </div>
                    <div class="image-info text-center">
                        <h5 class="title text-center">File NAME</h5>
                        <div class="text-center">
                            <a href="javascript:;"><i class="icon iconfont icon-shanchu"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
        </div>
        <!-- end panel -->
        </div>
        </form>
        <!-- end col-6 -->
        </div>
        <!-- end row -->
        <!-- begin row -->
        <div class="row hetong" style="display: none;">
        <form class="form-horizontal " id="form-contractinfo">
        <input type="hidden" value="" name="propertyId"/>
        <input type="hidden" value="" name="id"/>
        <!-- begin col-6 -->
        <div class="col-md-12">
        <!-- begin panel -->
        <div class="panel panel-inverse" data-sortable-id="form-stuff-1">
        <div class="panel-body">

        <h4>基本信息：</h4>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">管理单位:</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="rentManagementUnit" id="rentManagementUnit"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">负责人:</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="inChargePerson" id="inChargePerson"/>
                    </div>
                </div>
                <div class="col-md-4">
                    <label class="col-md-4 control-label">合同编号:</label>
                    <div class="col-md-8">
                        <input type="text" class="form-control" name="contractNo" id="contractNo"/>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-4">
                    <label class="col-md-4 control-label">合同流转号:</label>
