                                        <label class="col-md-2 control-label col-md-offset-2">产权情况</label>
                                        <div class="col-md-8">
                                            <select class="form-control" id="propertyRightCode" name="propertyRightCode">
                                                <option value="1">有产权证</option>
                                                <option value="2">无产权证</option>
                                                <option value="3">有使用权</option>
                                            </select>
                                        </div>
                                    </div>
