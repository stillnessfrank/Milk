    <script>
        var propertyService = abp.services.app.propertyAppServices;

        //百度地图宽度
        $('#l-map').width($(this).parent().width());
        // 百度地图API功能（新建）
        function G(id) {
            return document.getElementById(id);
        }

        var map = new BMap.Map("l-map");
        map.centerAndZoom("上海", 12);                   // 初始化地图,设置城市和地图级别。
        // map（新建）
        var ac = new BMap.Autocomplete(    //建立一个自动完成的对象
            {
                "input": "address"
                , "location": map
            });

        ac.addEventListener("onhighlight", function (e) {  //鼠标放在下拉列表上的事件
            var str = "";
            var _value = e.fromitem.value;
            var value = "";
            if (e.fromitem.index > -1) {
                value = _value.province + _value.city + _value.district + _value.street + _value.business;
            }
            str = "FromItem<br />index = " + e.fromitem.index + "<br />value = " + value;

            value = "";
            if (e.toitem.index > -1) {
                _value = e.toitem.value;
                value = _value.province + _value.city + _value.district + _value.street + _value.business;
            }
            str += "<br />ToItem<br />index = " + e.toitem.index + "<br />value = " + value;
            G("searchResultPanel").innerHTML = str;
        });

        var myValue;
        ac.addEventListener("onconfirm", function (e) {    //鼠标点击下拉列表后的事件
            var _value = e.item.value;
            myValue = _value.province + _value.city + _value.district + _value.street + _value.business;
            G("searchResultPanel").innerHTML = "onconfirm<br />index = " + e.item.index + "<br />myValue = " + myValue;

            setPlace();
        });

        function setPlace() {
            map.clearOverlays();    //清除地图上所有覆盖物
            function myFun() {
                var pp = local.getResults().getPoi(0).point;    //获取第一个智能搜索的结果
                map.centerAndZoom(pp, 18);
                map.addOverlay(new BMap.Marker(pp));    //添加标注
            }
            var local = new BMap.LocalSearch(map, { //智能搜索
                onSearchComplete: myFun
            });
            local.search(myValue);
            $.ajax({
                url: 'http://api.map.baidu.com/geocoder/v2/?address=' +
                    myValue +
                    '&output=json&ak=mGAIH1jhIoASGa67nHP3oMCXK3lGF8PB',
                dataType: 'jsonp',
                crossDomain: true,
                success: function (data) {
                    console.log(data);
                    $("#lng").val(data.result.location.lng);
                    $("#lat").val(data.result.location.lat);
                },
                err: function () {
                    console.log(data);
                }
            });
        }
        // map2

        function getTable() {
            var $dt = $('#data-table');
            //abp.ui.setBusy($dt);
            propertyService.getAllProperty().done(function (data) {
                console.log(data);
                if (data != null && data.length > 0) {
                    var tpl = _.template($('#dtTpl').html());
                    $dt.find("tbody").remove();
                    $dt.append("<tbody></tbody>");
                    $dt.find("tbody").append(tpl({ data: data }));
                } else {
                    $dt.find("tbody").remove();
                    $dt.append("<tbody></tbody>");
                }

                 $dt.DataTable({
                    "destroy": true,
                    "bInfo": true,
                    "pageLength": 10,
                    "columnDefs": [{
                        'targets': [12],
                        'orderable': false
                    }],
                    language: {
                        "sProcessing": "处理中...",
                        "sLengthMenu": "显示 _MENU_ 项结果",
                        "sZeroRecords": "没有匹配结果",
                        "sInfo": "显示第 _START_ 至 _END_ 项结果，共 _TOTAL_ 项",
                        "sInfoEmpty": "显示第 0 至 0 项结果，共 0 项",
                        "sInfoFiltered": "(由 _MAX_ 项结果过滤)",
                        "sInfoPostFix": "",
                        "sSearch": "搜索:",
                        "sUrl": "",
                        "sEmptyTable": "表中数据为空",
                        "sLoadingRecords": "载入中...",
                        "sInfoThousands": ",",
                        "oPaginate": {
                            "sFirst": "首页",
                            "sPrevious": "上页",
                            "sNext": "下页",
                            "sLast": "末页"
                        },
                        "oAria": {
                            "sSortAscending": ": 以升序排列此列",
                            "sSortDescending": ": 以降序排列此列"
                        }
                    },
                    autoFill: true,
                    responsive: true,
                    "lengthChange": false,   //选择每页显示多少条数据，下拉选项
                    "aLengthMenu": [[4, 10, 25, 100], [4, 10, 25, 100]] // 定义每页显示数据数量
                });

                $("#data-table_filter").hide();

                $dt.find("tbody").on("click", ".btn-edit", function () {
                    $("#editId").val($(this).attr("data-id"));
                    $("#edit").modal("show");

                });
                $dt.find("tbody").on("click", ".btn-delete", function () {
                    deleteItem($(this).attr("data-id"));
                });
                $dt.find("tbody").on("click", ".btn-room", function () {
                    location.href = "/PropertyMgr/AddRoom?propertyId=" + $(this).attr("data-id");
                });

            }).always(function () {
                //abp.ui.clearBusy($dt);
            });

        }

        function deleteItem(id) {
            abp.message.confirm(
                '是否删除资产?',
                '删除确认',
                function (isConfirmed) {
                    if (isConfirmed) {
                        abp.ui.setBusy("body");
                        var input = { id: id };
                        propertyService.deleteProperty(input).done(function () {
                            abp.message.success('删除资产成功！', '操作成功');
                            getTable();
                        }).always(function () {
                            abp.ui.clearBusy("body");
                        });
                    }
                }
            );
        }

        $(document).ready(function () {
            $("#global_search").hide();
            getTable();
            //TableManageAutofill.init();
            TableManageAutofill2.init();
            App.init();
            $('.sidebar-minify-btn').click().hide();
            FormWizard.init();
            FormWizard1.init();
            $('.well').height($('body').height() * 0.6).css("overflow", "auto");
            $(window).resize(function () {          //当浏览器大小变化时
                $('.well').height($('body').height() * 0.5).css("overflow", "auto");
            });
            FormPlugins.init();

            var $baseinfoForm = $("#form-baseinfo");
            var $propertyinfoForm = $("#form-propertyinfo");
            var $contractinfoForm = $("#form-contractinfo");

            $baseinfoForm.validate();
            $propertyinfoForm.validate();
            $contractinfoForm.validate();

            $("#edit").on('shown.bs.modal',
                function () {
                    var id = $("#editId").val();
                    var input = { id: id };
                    propertyService.getPropertyById(input).done(function (data) {
                        if (data != null) {
                            $("#form-baseinfo").find("input,select,textarea").each(function(i, obj) {
                                $(obj).val(data[$(obj).attr("name")]);
                            });
                            if (data.contractInfo != null) {
                                $("#form-contractinfo").find("input,select,textarea").each(function (i, obj) {
                                    $(obj).val(data.contractInfo[$(obj).attr("name")]);
                                });
                            }
                            if (data.propertyInfo != null) {
                                $("#form-propertyinfo").find("input,select,textarea").each(function (i, obj) {
                                    $(obj).val(data.propertyInfo[$(obj).attr("name")]);
                                });
                            }

                            setPlace();
                        }
                    });
                });

            $("#btnSave").on("click", function () {
                var id = $("#editId").val();
                if (!$baseinfoForm.valid()) {
                    return;
                }
                var baseinfo = $baseinfoForm.serializeFormToObject();
                baseinfo.id = id;
                if (baseinfo.propertyRightCode == "3") {
                    if (!$contractinfoForm.valid()) {
                        return;
                    }
                    var contractInfo = $contractinfoForm.serializeFormToObject();
                    baseinfo.contractInfo = contractInfo;
                } else {
                    if (!$propertyinfoForm.valid()) {
                        return;
                    }
                    var propertyInfo = $propertyinfoForm.serializeFormToObject();
                    baseinfo.propertyInfo = propertyInfo;
                }
                abp.ui.setBusy($("body"));
                propertyService.updateProperty(baseinfo).done(function () {
                    abp.message.success('编辑资产成功！', '操作成功');
                    $("input[type=text] textarea").val("");
                    $("#edit").modal("hide");
                    getTable();
                }).always(function () {
                    abp.ui.clearBusy($("body"));
                });
            });

            $("#inputSearch").on("keyup", function () {
                $('#data-table').DataTable().search($("#inputSearch").val()).draw();
            });
        });
    </script>
}
